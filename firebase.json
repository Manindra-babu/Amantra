{
  "hosting": {
    "site": "amantra-360",
    "public": "public",
    "ignore": [
      "firebase.json",
      "public/js/mock_data.js",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/dashboard.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}